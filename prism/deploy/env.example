# ./deploy/env.example - v1.0.0
# Copy to .env (do not commit real secrets)
ENV=dev
API_HOST=0.0.0.0
API_PORT=8000
API_KEY=dev-key-change-me
MOCK_MODE=true
DATA_DIR=/app/data
LOG_LEVEL=INFO
ALLOW_ORIGINS=*

# OpenAI / LLM (optional when MOCK_MODE=true)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Notion/MCP (mock by default)
NOTION_API_KEY=
MCP_ENDPOINT=
EOF ./deploy/env.example - v1.0.0
